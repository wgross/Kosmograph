<Window x:Class="Kosmograph.Desktop.Dialogs.EditTagDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kosmograph.Desktop.Dialogs"
        mc:Ignorable="d"
        Title="EditTagDialog" Height="450" Width="800">
    <DockPanel LastChildFill="False">
        <Label DockPanel.Dock="Top">Name:</Label>
        <TextBox DockPanel.Dock="Top" Text="{Binding Name}"></TextBox>
    </DockPanel>
</Window>